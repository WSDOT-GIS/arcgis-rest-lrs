{
  "name": "leaflet-demo",
  "version": "1.0.0-rc.1",
  "description": "Demo application using ArcGIS REST LRS client",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:css && npm run build:site",
    "build:css": "lessc style/index.less style/index.css",
    "build:site": "webpack",
    "watch:site": "webpack -w",
    "build:utils": "tsc -p util",
    "copy:leaflet": "node util/copyLeaflet.js",
    "prettier": "prettier \"{src,util,style}/**/*.{ts,less}\" --write",
    "serve": "dotnet serve",
    "gh-pages": "npm run build && node util/publish.js"
  },
  "keywords": [
    "lrs",
    "arcgis"
  ],
  "private": true,
  "author": {
    "name": "Jeff Jacobson",
    "url": "https://github.com/JeffJacobson"
  },
  "license": "Unlicense",
  "devDependencies": {
    "@esri/arcgis-rest-request": "^4.0.3",
    "@types/arcgis-rest-api": "^10.4.5",
    "@types/esri-leaflet": "^2.1.9",
    "@types/fs-extra": "^9.0.13",
    "@types/geojson": "^7946.0.8",
    "@types/leaflet": "^1.7.11",
    "@types/node": "^18.0.3",
    "@wsdot/arcgis-rest-lrs": "^1.0.0-rc.1",
    "esri-leaflet": "^3.0.8",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "leaflet": "^1.8.0",
    "less": "^4.1.3",
    "prettier": "^2.7.1",
    "source-map": "^0.7.4",
    "ts-loader": "^9.3.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
