{
  "name": "leaflet-demo",
  "version": "1.0.0-rc.1",
  "description": "Demo application using ArcGIS REST LRS client",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:css && npm run build:site",
    "build:css": "lessc style/index.less style/index.css",
    "build:site": "webpack",
    "watch:site": "webpack -w",
    "build:utils": "tsc -p util",
    "copy:leaflet": "node util/copyLeaflet.js",
    "prettier": "prettier \"{src,util,style}/**/*.{ts,less}\" --write",
    "gh-pages": "npm run build && node util/publish.js",
    "serve": "dotnet serve"
  },
  "keywords": [
    "lrs",
    "arcgis"
  ],
  "private": true,
  "author": {
    "name": "Jeff Jacobson",
    "url": "https://github.com/JeffJacobson"
  },
  "license": "Unlicense",
  "devDependencies": {
    "@esri/arcgis-rest-request": "^4.0.3",
    "@types/arcgis-rest-api": "^10.4.5",
    "@types/esri-leaflet": "^2.1.9",
    "@types/fs-extra": "^11.0.1",
    "@types/geojson": "^7946.0.10",
    "@types/leaflet": "^1.9.0",
    "@types/node": "^18.11.18",
    "@wsdot/arcgis-rest-lrs": "^1.0.0-rc.4",
    "esri-leaflet": "^3.0.10",
    "fs-extra": "^11.1.0",
    "gh-pages": "^5.0.0",
    "leaflet": "^1.9.3",
    "less": "^4.1.3",
    "prettier": "^2.8.3",
    "source-map": "^0.7.4",
    "ts-loader": "^9.4.2",
    "tslib": "^2.5.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
