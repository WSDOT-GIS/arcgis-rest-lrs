{
  "name": "leaflet-demo",
  "version": "1.0.0-rc.0",
  "description": "Demo application using ArcGIS REST LRS client",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:css && npm run build:site",
    "build:css": "lessc style/index.less style/index.css",
    "build:site": "webpack",
    "watch:site": "webpack -w",
    "build:utils": "tsc -p util",
    "copy:leaflet": "node util/copyLeaflet.js",
    "prettier": "prettier \"{src,util,style}/**/*.{ts,less}\" --write",
    "gh-pages": "npm run build && node util/publish.js"
  },
  "keywords": [
    "lrs",
    "arcgis"
  ],
  "private": true,
  "author": {
    "name": "Jeff Jacobson",
    "url": "https://github.com/JeffJacobson"
  },
  "license": "Unlicense",
  "devDependencies": {
    "@esri/arcgis-rest-request": "^1.1.1",
    "@types/arcgis-rest-api": "^10.4.3",
    "@types/esri-leaflet": "^2.1.2",
    "@types/fs-extra": "^5.0.1",
    "@types/geojson": "^7946.0.2",
    "@types/leaflet": "^1.2.6",
    "@types/node": "^9.4.7",
    "@wsdot/arcgis-rest-lrs": "^1.0.0-rc.0",
    "esri-leaflet": "^2.1.4",
    "fs-extra": "^5.0.0",
    "gh-pages": "^1.1.0",
    "leaflet": "^1.3.1",
    "less": "^3.0.1",
    "prettier": "^1.11.1",
    "source-map": "^0.7.2",
    "ts-loader": "^4.1.0",
    "tslib": "^1.9.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/arcgis-rest-lrs-demo.git"
  },
  "bugs": {
    "url": "https://github.com/WSDOT-GIS/arcgis-rest-lrs-demo/issues"
  },
  "homepage": "https://github.com/WSDOT-GIS/arcgis-rest-lrs-demo#readme"
}
