    "@esri/arcgis-rest-request": "^2.0.0"
  },
  "devDependencies": {
    "@esri/arcgis-rest-request": "^2.0.0",
    "@types/arcgis-rest-api": "^10.4.4",
    "@types/fetch-mock": "^7.2.3",
    "@types/jasmine": "^3.3.12",
    "fetch-mock": "^7.3.3",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^2.0.0",
    "jasmine": "^3.4.0",
    "prettier": "^1.17.0",
    "source-map": "^0.7.3",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/arcgis-rest-lrs.git"
  },
