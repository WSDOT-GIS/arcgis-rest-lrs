  "dependencies": {
    "tslib": "^2.4.0"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-request": "^4.0.3"
  },
  "devDependencies": {
    "@esri/arcgis-rest-request": "^4.0.3",
    "@types/arcgis-rest-api": "^10.4.5",
    "@types/fetch-mock": "^7.3.5",
    "@types/jasmine": "^4.0.3",
    "fetch-mock": "^9.11.0",
    "jasmine": "^4.2.1",
    "prettier": "^2.7.1",
    "source-map": "^0.7.4",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WSDOT-GIS/arcgis-rest-lrs.git"
  },
  "bugs": {
